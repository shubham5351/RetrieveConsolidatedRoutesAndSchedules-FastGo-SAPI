#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    departureDateTime:
      required: true
      type: string
      example: "2022-02-23T20:45:30"
    fromLocation:
      required: true
      type: string
      example: "BEN-SG"
    toLocation:
      required: true
      type: string
      example: "PUN-SG"
    availableSeats:
      required: true
      type: number
      example: 12